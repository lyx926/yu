	    	params = params+"&functionId="+functionId;
	    }	    
		 $.ajax({
			url:exchangePath+"/f_exchange/getSubType.shtml",
			data:params,
			cache:false,
			type:"post",
			dataType:"json",
			success:function(data) {
				if(typeof(data.success) == 'undefined')
				{
